{{yield}}

<div class="container">
  <table class="table table-bordered">
    <thead>
      <tr>
        <th colspan="9" text-allign="center">Time Table</th>
      </tr>
      <tr>
        <th>Day</th>
        <th>Time Slot 1</th>
        <th>Time Slot 2</th>
        <th>Time Slot 3</th>
        <th>Time Slot 4</th>
        <th>Time Slot 5</th>
        <th>Time Slot 6</th>
        <th>Time Slot 7</th>
        <th>Time Slot 8</th>
      </tr>
    </thead>
    <tbody>
      {{#each model as |a| }}
      <tr>
        <td>{{splitDayFromId a.uid}}</td>
        <td>{{a.time_slot1}}</td>
        <td>{{a.time_slot2}}</td>
        <td>{{a.time_slot3}}</td>
        <td>{{a.time_slot4}}</td>
        <td>{{a.time_slot5}}</td>
        <td>{{a.time_slot6}}</td>
        <td>{{a.time_slot7}}</td>
        <td>{{a.time_slot8}}</td>
      </tr>
      {{/each}}
    </tbody>
  </table>
</div>
